{
    "manifestVersion": 1,
    "id": "CodeCoverageThresholds",
    "name": "Code Coverage Thresholds",
    "version": "1.0.11",
    "publisher": "LakshayKaushik",
    "public": true,
    "repository": {
        "type": "git",
        "uri": "https://github.com/lakshaykaushik/AzdoCodeCoverageThreshold"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Lets you define code Coverage threshold on project level",
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/codecoverage.png"        
    },
    "files": [
        {
            "path": "codecoveragethreshold"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "codecoveragethreshold"
            }
        }
    ]
}